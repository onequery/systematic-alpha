# Required credentials
KIS_APP_KEY=your_app_key
KIS_APP_SECRET=your_app_secret
KIS_ACC_NO=12345678-01

# Optional
KIS_USER_ID=
KIS_MOCK=0

# Telegram notification (optional)
# If TELEGRAM_ENABLED is empty, notification auto-enables when both token/chat id are set.
# TELEGRAM_ENABLED=1
# TELEGRAM_BOT_TOKEN=123456789:ABCDEFGHIJKLMNOPQRSTUVWXYZ123456789  # no leading "bot"
# TELEGRAM_CHAT_ID=123456789
# TELEGRAM_THREAD_ID=
# TELEGRAM_DISABLE_NOTIFICATION=0

# Strategy / runtime overrides (optional)
# UNIVERSE_CODES_FILE=./codes.txt
# COLLECT_SECONDS=600
# MAX_SYMBOLS_SCAN=400
# PRE_CANDIDATES=40
# FINAL_PICKS=3
# MIN_CHANGE_PCT=3.0
# MIN_GAP_PCT=2.0
# MIN_PREV_TURNOVER=10000000000
# MIN_STRENGTH=100.0
# MIN_VOL_RATIO=0.10
# MIN_BID_ASK_RATIO=1.2
# MIN_PASS_CONDITIONS=5
# MIN_MAINTAIN_RATIO=0.6
# REST_SLEEP_SEC=0.03
# STAGE1_LOG_INTERVAL=20
# REALTIME_LOG_INTERVAL=10
# SELECTION_OUTPUT_JSON=./out/picks.json
