# Sensitive credentials only.
# Non-sensitive runtime config is managed in config/agent_lab.config.
# Copy this file to .env and fill your real secrets.

# KIS credentials (required)
KIS_APP_KEY=your_app_key
KIS_APP_SECRET=your_app_secret
KIS_ACC_NO=12345678-01

# Optional sensitive identifier
KIS_USER_ID=

# OpenAI API key (required for LLM-enabled workflows)
OPENAI_API_KEY=sk-...

# Telegram bot secrets (optional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
TELEGRAM_THREAD_ID=

# For runtime settings, copy config/agent_lab.config.example to config/agent_lab.config.
